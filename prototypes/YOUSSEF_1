#include <stdio.h>
#include "brick.h"

#define M_LEFT   OUTB
#define M_RIGHT OUTC
#define M_BOTH  ( M_RIGHT | M_LEFT ) 
#define US_SENSOR OUT?


Int main()
{   
    int max_speed;


    if(!brick_int()) return(1);
    printf("EV3 says HELLO");
    
    if( tacho_is_pluged(M_BOTH; TACHO_TYPE__NONE_));
    {
        max_speed = tacho_get_max_speed( M_RIGHT, 0);
        tacho_reset( M_BOTH);
    }
    else
    {
        printf("plug the motors");
        brick_uninit();
        return(0);
    }
    tacho_set_speed_sp( M_BOTH, max_speed * 0.25 );

    tacho_run_forever(  M_BOTH );
    Sleep( 1000 );
    tacho_stop( M_LEFT);
    sleep(2000);
    tacho_stop( M_RIGHT);
    sleep(2000);

}